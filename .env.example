# Sentinel Agent v2.0 - Environment Configuration
# Copy this file to .env and fill in your values

# Google Gemini API Key (REQUIRED)
# Get your key from: https://aistudio.google.com/app/apikeys
GEMINI_API_KEY=your_gemini_api_key_here

# Web API Configuration (Optional)
API_PORT=8080
API_JWT_SECRET=your_random_secret_here

# Game Automation Settings
GAME_TYPE=Roblox
AUTO_APPROVE=false

# Logging
LOG_LEVEL=info
SANDBOX_DIR=./sandbox
